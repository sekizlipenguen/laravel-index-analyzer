{
    "name": "sekizlipenguen/laravel-index-analyzer",
    "description": "Advanced package that automatically analyzes Laravel SQL queries and provides intelligent index suggestions to optimize database performance",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sekizli Penguen",
            "homepage": "https://github.com/sekizlipenguen"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^9.0|^10.0|^11.0|^12.0",
        "ext-pdo": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0",
        "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "SekizliPenguen\\IndexAnalyzer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SekizliPenguen\\IndexAnalyzer\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SekizliPenguen\\IndexAnalyzer\\IndexAnalyzerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
