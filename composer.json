{
    "name": "sekizlipenguen/laravel-index-analyzer",
    "description": "Laravel SQL sorguları için otomatik index önerisi sunan gelişmiş bir paket",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sekizli Penguen",
            "email": "info@sekizlipenguen.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "laravel/framework": "^8.0|^9.0|^10.0",
        "ext-pdo": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0",
        "orchestra/testbench": "^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "SekizliPenguen\\LaravelIndexAnalyzer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SekizliPenguen\\LaravelIndexAnalyzer\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SekizliPenguen\\LaravelIndexAnalyzer\\LaravelIndexAnalyzerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
